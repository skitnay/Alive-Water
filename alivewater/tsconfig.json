{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "components/Header.js", "components/Footer.js", "components/Carusel.js", "components/Addresses.js", "components/Layout.js", "components/Modal.js", "components/New.js", "components/Waterpumps.js", "pages/_app.js", "pages/_document.js", "pages/analyzes.js", "pages/contacts.js", "pages/index.js", "pages/news.js", "pages/waterpumps.js"  ],
  "exclude": [
    "node_modules"
  ]
}
